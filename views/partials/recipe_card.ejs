<div class="card">
    <img src=<%= data[index][0] %> class="card_img">
    <div class= "card_container_info">
        <h2 class="card_title"><%= data[index][0] %></h2>
        <ul class="card_info">
            <li><%= data[index][0] %> min</li>
            <li>Serving: <%= data[index][0] %></li>
        </ul>
        <form action="/recipe" method="POST">
            <input class="card_id" type="id" name="id" value=<%= data[index][0] %>>
            <input class="card_link" type="submit" value="View Recipe">
        </form>
        <p><%= data[index][0] %></p>
    </div>
</div>